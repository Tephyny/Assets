  "scripts": {
    "start": "electron .",
    "dist": "electron-builder --linux deb"
  },
  "author": {
    "name": "Stephanie Shawa",
    "email": "sshawa48@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "chart.js": "^4.4.4",
